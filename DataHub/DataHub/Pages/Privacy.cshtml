@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to initialize your Username: @Model.Name</p>

<div class="container">
    <form method="POST" action="">
        <input type="text" asp-for="Name" name="username" placeholder="Username">
        <button type="submit">Submit @Model.Name</button>
    </form>
</div>

